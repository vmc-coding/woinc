#include <woinc/version.h>

namespace woinc {

    int major_version() {
        return @WOINC_VERSION_MAJOR@;
    }

    int minor_version() {
        return @WOINC_VERSION_MINOR@;
    }

    int boinc_major_version() {
        return @WOINC_BOINC_VERSION_MAJOR@;
    }

    int boinc_minor_version() {
        return @WOINC_BOINC_VERSION_MINOR@;
    }

    int boinc_release_version() {
        return @WOINC_BOINC_VERSION_RELEASE@;
    }

}
